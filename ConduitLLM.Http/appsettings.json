{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "ConduitLLM.Core.Services": "Debug",
      "ConduitLLM.Core.Services.EventPublishingServiceBase": "Debug",
      "ConduitLLM.Core.Services.VideoGenerationOrchestrator": "Debug",
      "MassTransit": "Debug"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "RedisSignalR": "redis:6379,abortConnect=false,connectTimeout=5000,syncTimeout=5000"
  },
  "ConduitLLM": {
    "RabbitMQ": {
      "Host": "",
      "Port": 5672,
      "Username": "guest",
      "Password": "guest",
      "VHost": "/",
      "PrefetchCount": 10,
      "PartitionCount": 10,
      "Durable": true,
      "AutoDelete": false,
      "ConnectionRetryCount": 5,
      "HeartbeatInterval": 60,
      "NetworkRecoveryInterval": 10,
      "AutomaticRecoveryEnabled": true
    },
    "MediaManagement": {
      "EnableOwnershipTracking": true,
      "EnableAutoCleanup": true,
      "MediaRetentionDays": 90,
      "OrphanCleanupEnabled": true,
      "AccessControlEnabled": false
    },
    "NotificationBatching": {
      "EnableBatching": true,
      "DefaultBatchingWindowSeconds": 5,
      "MaxBatchingDelaySeconds": 10,
      "MaxBatchSize": 100,
      "ImmediateSeverityLevels": ["Critical"]
    }
  }
}