# Conduit API Configuration
NEXT_PUBLIC_CONDUIT_ADMIN_API_URL=http://localhost:5002
NEXT_PUBLIC_CONDUIT_CORE_API_URL=http://localhost:5000
CONDUIT_MASTER_KEY=your-master-key-here
CONDUIT_WEBUI_AUTH_KEY=your-webui-auth-key-here

# Development Configuration
NODE_ENV=development
NEXT_PUBLIC_APP_ENV=development

# SignalR Configuration
NEXT_PUBLIC_SIGNALR_AUTO_RECONNECT=true
NEXT_PUBLIC_SIGNALR_RECONNECT_INTERVAL=5000

# Feature Flags
NEXT_PUBLIC_ENABLE_REAL_TIME_UPDATES=true
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_DEBUG_MODE=false