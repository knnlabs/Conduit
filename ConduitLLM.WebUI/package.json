    "husky": "^9.0.11",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "lint-staged": "^16.1.2",
    "playwright": "^1.54.1",
    "ts-jest": "^29.4.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
