{
  "name": "conduit-webui",
  "version": "1.0.0",
  "description": "Next.js WebUI for Conduit LLM Platform",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "lint:fix": "next lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare": "cd .. && husky install ConduitLLM.WebUI/.husky",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^18.0.1",
    "@knn_labs/conduit-admin-client": "file:../Clients/Node/Admin",
    "@knn_labs/conduit-core-client": "file:../Clients/Node/Core",
    "@mantine/carousel": "^8.1.2",
    "@mantine/charts": "^8.1.2",
    "@mantine/code-highlight": "^8.1.2",
    "@mantine/core": "^8.1.2",
    "@mantine/dates": "^8.1.2",
    "@mantine/form": "^8.1.2",
    "@mantine/hooks": "^8.1.2",
    "@mantine/modals": "^8.1.2",
    "@mantine/notifications": "^8.1.2",
    "@mantine/spotlight": "^8.1.2",
    "@tabler/icons-react": "^3.34.0",
    "@tanstack/react-query": "^5.0.0",
    "@types/node": "^24.0.7",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "axios": "^1.10.0",
    "eslint": "^9.30.0",
    "next": "^15.3.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "typescript": "^5.8.3",
    "zod": "^3.25.72",
    "zustand": "^5.0.6"
  },
  "keywords": [
    "conduit",
    "llm",
    "nextjs",
    "react",
    "typescript",
    "mantine"
  ],
  "author": "KNN Labs",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "@types/jest": "^30.0.0",
    "eslint-config-next": "^15.3.5",
    "husky": "^9.0.11",
    "jest": "^30.0.4",
    "lint-staged": "^15.2.2",
    "playwright": "^1.54.1",
    "ts-jest": "^29.4.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "tsc --noEmit"
    ],
    "*.{js,jsx}": [
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
