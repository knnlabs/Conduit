{
  "name": "@knn_labs/conduit-common",
  "version": "0.2.0",
  "description": "Common types and utilities for Conduit SDK clients",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "conduit",
    "llm",
    "types",
    "sdk"
  ],
  "author": "KNN Labs",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.0.15",
    "tsup": "^8.1.0",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "typescript": ">=4.5.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "dependencies": {
    "@microsoft/signalr": "^8.0.7"
  }
}
