{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AdminApi": {
    "MasterKey": "your-master-key-here", 
    "AllowedOrigins": [ "http://localhost:5000", "https://localhost:5001" ]
  },
  "ConnectionStrings": {
    "ConfigurationDb": "Data Source=conduit.db"
  },
  "ConduitLLM": {
    "RabbitMQ": {
      "Host": "",
      "Port": 5672,
      "Username": "guest",
      "Password": "guest",
      "VHost": "/",
      "PrefetchCount": 10,
      "PartitionCount": 10,
      "Durable": true,
      "AutoDelete": false,
      "ConnectionRetryCount": 5,
      "HeartbeatInterval": 60,
      "NetworkRecoveryInterval": 10,
      "AutomaticRecoveryEnabled": true
    }
  }
}