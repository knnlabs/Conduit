{
  "name": "@knn_labs/conduit-admin-client",
  "version": "1.0.1-dev.20250721212505",
  "description": "TypeScript client library for Conduit Admin API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./nextjs": {
      "types": "./dist/nextjs.d.ts",
      "import": "./dist/nextjs.mjs",
      "require": "./dist/nextjs.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build && npm run test",
    "publish:dev": "npm version prerelease --preid=dev && npm publish --tag dev",
    "publish:stable": "npm publish --tag latest",
    "version:dev": "npm version prerelease --preid=dev --no-git-tag-version",
    "version:patch": "npm version patch --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:major": "npm version major --no-git-tag-version"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "conduit",
    "llm",
    "api",
    "client",
    "typescript",
    "admin",
    "knn-labs"
  ],
  "author": "Conduit Team",
  "license": "MIT",
  "peerDependencies": {
    "next": ">=13.0.0"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    }
  },
  "dependencies": {
    "@knn_labs/conduit-common": "file:../Common",
    "@microsoft/signalr": "^8.0.7",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.15",
    "@types/react": "^19.1.8",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "eslint": "^9.31.0",
    "jest": "^30.0.4",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/knnlabs/Conduit.git",
    "directory": "Clients/Node/Admin"
  },
  "homepage": "https://github.com/knnlabs/Conduit/tree/master/Clients/Node/Admin",
  "bugs": {
    "url": "https://github.com/knnlabs/Conduit/issues"
  }
}
